{
  "name": "sage",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "sst bind vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "prepare": "husky install",
    "sst:deploy": "sst deploy",
    "sst:dev": "sst dev"
  },
  "dependencies": {
    "marked": "^5.1.0",
    "openai": "^3.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.4.2",
    "@playwright/test": "^1.35.1",
    "@skeletonlabs/skeleton": "^1.8.0",
    "@sveltejs/kit": "^1.20.5",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/marked": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "^10.4.14",
    "aws-cdk-lib": "2.84.0",
    "constructs": "10.1.156",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.31.1",
    "highlight.js": "^11.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "sst": "^2.16.3",
    "svelte": "^3.59.2",
    "svelte-check": "^3.4.4",
    "svelte-kit-sst": "^2.16.3",
    "tailwind-highlightjs": "^2.0.1",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.25.8"
  },
  "type": "module",
  "lint-staged": {
    "*.{ts,js,css,postcss,html,json,yaml,svelte,md}": [
      "prettier --plugin-search-dir . --write",
      "pnpm vitest run --silent --",
      "pnpm check",
      "pnpm eslint --fix",
      "pnpm build --"
    ]
  }
}
